{
  "name": "sdk-akademie",
  "version": "0.0.0",
  "scripts": {
    "start": "ng serve",
    "build": "ng build",
    "build:prod": "ng build --configuration=production",
    "build:ssr": "ng build && ng run sdk-akademie:server",
    "serve:ssr": "node dist/sdk-akademie/server/main.js",
    "dev": "ng serve",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "cypress run",
    "cypress:open": "cypress open",
    "extract-i18n": "ng extract-i18n",
    "deploy": "ng deploy",
    "start:ssr": "ng run sdk-akademie:serve-ssr",
    "start:json-server": "json-server --watch src/assets/data/courses-details.json",
    "optimize-images": "node scripts/optimize-images.js",
    "inline-critical-css": "node scripts/inline-critical-css.js",
    "performance-test": "npm run build:prod && npm run inline-critical-css && npm run analyze-bundle",
    "lighthouse": "npx lighthouse http://localhost:4000 --output=html --output-path=./lighthouse-report.html"
  },
  "private": true,
